{
  "name": "@x-ear/web",
  "version": "0.1.0",
  "description": "X-EAR Web Application",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run type-check && vite --port 8080",
    "dev:typecheck": "npm run type-check && vite --port 8080",
    "dev:fast": "vite --port 8080",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:report": "playwright show-report",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "gen:api": "orval && node scripts/generate-api-aliases.mjs && node scripts/generate-api-index.mjs",
    "gen:key": "node scripts/gen-storage-key.cjs"
  },
  "dependencies": {
